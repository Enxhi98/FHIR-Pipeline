import pandas as pd
import json
json_data = """{
  "entry": [ {
    "resource": {
      "extension": [ {
        "extension": [ {
          "valueCoding": {
            "display": "White"
          }
        } ]
      } 
      ]}}]}"""



parsed_json = (json.loads(json_data))
#df = pd.json_normalize(parsed_json["entry"])
df = pd.DataFrame.from_records(parsed_json["entry"], columns=["resource"])

print(df)

